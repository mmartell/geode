<?xml version="1.0"?>

<cache xmlns="http://gemfire.apache.org/schema/cache"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://gemfire.apache.org/schema/cache http://gemfire.apache.org/schema/cache/cache-1.0.xsd"
       version="1.0">
  <cache-server port="HOST_PORT1">
  <group>A</group>
  </cache-server>
  <disk-store name="DEFAULT" auto-compact="true" max-oplog-size="20">
      <disk-dirs>
        <disk-dir>../backupDirectory1</disk-dir>
      </disk-dirs>
  </disk-store>

  <region name="DistRegionAck" refid="REPLICATE_PERSISTENT"/>
</cache>
