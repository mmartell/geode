<?xml version="1.0"?>


<cache xmlns="http://gemfire.apache.org/schema/cache"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://gemfire.apache.org/schema/cache http://gemfire.apache.org/schema/cache/cache-1.0.xsd"
       version="1.0">
	<cache-server port="HOST_PORT1">	
	<group>A</group>
	<group>AB</group>
	<group>ABC</group>
	</cache-server>
	<region name="DistRegionAck">
		<region-attributes data-policy="partition">
		<partition-attributes redundant-copies="1"/>
          <cache-writer>
            <class-name>javaobject.CacheWriterForSingleHop</class-name>
          </cache-writer>

        </region-attributes>
	</region>
	<region name="DistRegionNoAck">
		<region-attributes data-policy="partition">
		<partition-attributes redundant-copies="1"/>			   
      <cache-writer>
        <class-name>javaobject.CacheWriterForSingleHop</class-name>
      </cache-writer>

      </region-attributes>
	</region>
</cache> 
