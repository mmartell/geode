<?xml version="1.0"?>

<!-- serverDurableClient.xml
     Configures a server to for clients at port 40404.
-->

<cache xmlns="http://gemfire.apache.org/schema/cache"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://gemfire.apache.org/schema/cache http://gemfire.apache.org/schema/cache/cache-1.0.xsd"
       version="1.0">
  <cache-server port="40404"/>
    <region name="DistRegionAck">
      <region-attributes scope="distributed-ack" data-policy="replicate"/>
    </region>
</cache>
