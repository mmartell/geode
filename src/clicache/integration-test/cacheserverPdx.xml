<?xml version="1.0"?>


<cache xmlns="http://gemfire.apache.org/schema/cache"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://gemfire.apache.org/schema/cache http://gemfire.apache.org/schema/cache/cache-1.0.xsd"
       version="1.0">
	<!--cache-server host="cod" port="24680" /-->
  <cache-server port="HOST_PORT1"/>
  <pdx read-serialized="true" />  
	<region name="DistRegionAck">
		<region-attributes scope="distributed-ack" data-policy="replicate">
     <cache-listener>
				<class-name>javaobject.PdxCacheListener</class-name>
			</cache-listener>
    </region-attributes>
	</region>
	<region name="DistRegionNoAck">
		<region-attributes   scope="distributed-no-ack" data-policy="replicate">
    <cache-listener>
				<class-name>javaobject.PdxCacheListener</class-name>
			</cache-listener>
    </region-attributes>
	</region>
	<region name="testregion">
		<region-attributes   scope="distributed-no-ack" data-policy="replicate">
    <cache-listener>
				<class-name>javaobject.PdxCacheListener</class-name>
			</cache-listener>
    </region-attributes>
	</region>
</cache> 
